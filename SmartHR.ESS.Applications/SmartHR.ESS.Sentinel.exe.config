<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings file="user.config">
    <add key="DbRequestTimeout" value="120"/>
    <add key="AppName" />
    <add key="DbName" />
    <add key="DbServerName" />
    <add key="Delay" value="5" />
    <add key="MaxJobs" value="10" />
    <add key="MaxMessageCount" />
    <add key="ReconnectWaitTime" />
    <add key="EmailEnableSsl" value="1" />
  </appSettings>
  <connectionStrings configSource="connection.config" />
</configuration>